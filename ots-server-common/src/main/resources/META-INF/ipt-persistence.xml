<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0">

  <persistence-unit name="iptModel" transaction-type="RESOURCE_LOCAL">
    <non-jta-data-source>otsDataSource</non-jta-data-source>
    <shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
    <validation-mode>AUTO</validation-mode>

    <!-- IPT ENTITY CLASSES -->
    
    
    <class>com.fuso.enterprise.ots.srv.server.model.entity.OtsUsers</class>
    <class>com.fuso.enterprise.ots.srv.server.model.entity.OtsRegistration</class>
    <class>com.fuso.enterprise.ots.srv.server.model.entity.OtsUserRole</class>
    <class>com.fuso.enterprise.ots.srv.server.model.entity.OtsUserMapping</class>
   
    
    <!---OTS ORM FILES -->

   
	
  </persistence-unit>

</persistence>