<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey_300"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/slNo"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="Sl No"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/customerName"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/customername"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/orderNo"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/ordernumber"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/deliveredDate"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/delivereddate"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/productName"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/productname"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/orderedQty"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/orderquantity"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/deliveredQty"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/deliveryquantity"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/emptyCanRecieved"
            android:layout_width="@dimen/activity_margin_150"
            android:layout_height="wrap_content"
            android:text="@string/emptycan"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/balanceCan"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/balancecan"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/productCost"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/ordercost"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/cashRecieved"
            android:layout_width="@dimen/activity_margin_100"
            android:layout_height="wrap_content"
            android:text="@string/cansreceived"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/grey_300"
            />
        <TextView
            android:id="@+id/amountOutstanding"
            android:layout_width="@dimen/activity_margin_150"
            android:layout_height="wrap_content"
            android:text="@string/outstandingamoount"
            android:gravity="center"
            android:padding="@dimen/activity_margin_4"
            />
    </LinearLayout>
</LinearLayout>